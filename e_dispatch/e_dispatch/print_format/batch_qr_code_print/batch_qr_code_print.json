 "font_size": 14,
 "html": "<!DOCTYPE html>\r\n<html>\r\n    <body>\r\n        {% for item in doc.qrcode_details %}\r\n        \r\n            \r\n                {% if loop.index % 3 == 1%}\r\n                    <div class=\"row\" style=\"min-width: 190mm important; margin-left: 0px; margin-right: 0px; height:30mm important;\">\r\n                {% endif %}\r\n                    <div class=\"col-xs-4\" style=\"padding-left: 11px; padding-right: 0px; padding-top: 7px; padding-bottom: 7px; important\">\r\n                    <div class=\"col-xs-6\" style=\"padding-left: 0px; padding-right: 0px; padding-bottom: 0px; important\">\r\n                            <img  src= \"{{ item.content }}\">\r\n                        </div><br>\r\n                        <div class=\"col-xs-6 text-left\" style=\"font-size: 10px; padding-left: 0px; padding-right: 0px; padding-bottom: 0px; important\">\r\n                            <p>{{ doc.item }} </p>\r\n                            <p>{{ item.qr_code_id }} </p>\r\n                            <p>{{ frappe.db.get_value(\"Item\", doc.production_item, \"qty_per_box\") }} </p>\r\n                            {% set wo_details = frappe.db.get_value(\"Work Order\", doc.name, [\"planned_start_date\", \"fg_warehouse\"], as_dict=1) %}\r\n                            {% if wo_details %}\r\n                                <p>{{ frappe.utils.getdate(wo_details.planned_start_date) }} </p>\r\n                                <p>{{ wo_details.fg_warehouse }} </p>\r\n                            {% else %}\r\n                                <p>{{ item.warehouse }} </p>\r\n                            {% endif %}\r\n                        </div>\r\n                    </div>    \r\n                {% if loop.index % 3 == 0%}\r\n                    </div>\r\n                {% endif %}\r\n                \r\n            \r\n            {% if loop.index % 24 == 0%}\r\n                <div style=\"page-break-before: always;\"></div>\r\n                <div style=\"padding-top:4mm;\"> </div>\r\n            {% endif %}\r\n        {%- endfor -%}\r\n    </body>\r\n</html>\r\n",
